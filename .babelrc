{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "firefox >= 63"
          ]
        },
        "useBuiltIns": "usage",
        "modules": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-object-rest-spread",
    "@babel/plugin-syntax-async-generators"
  ],
  "env": {
    "production": {
      "presets": [
        "minify"
      ]
    },
    "test": {
      "plugins": [
        "istanbul",
        "rewire"
      ]
    }
  }
}
